{
  "name": "locklift",
  "version": "1.33.7",
  "description": "Node JS framework for working with FreeTON contracts. Inspired by Truffle and Hardhat. Helps you to build, test, run and maintain your smart contracts.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc"
  },
  "bin": {
    "locklift": "build/cli/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "build/index.d.ts"
      ],
      "config": [
        "build/config/index.d.ts"
      ],
      "contract": [
        "build/contract/index.d.ts"
      ],
      "factory": [
        "build/factory/index.d.ts"
      ],
      "giver": [
        "build/giver/index.d.ts"
      ],
      "ton": [
        "build/ton/index.d.ts"
      ],
      "utils": [
        "build/utils/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/broxus/ton-locklift.git"
  },
  "keywords": [
    "FreeTON",
    "TON",
    "blockchain",
    "smart-contracts",
    "javascript",
    "solidity",
    "dapps",
    "ethereum"
  ],
  "author": "pavlovdog_ <pavlovdog.moscow@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/broxus/ton-locklift/issues"
  },
  "homepage": "https://github.com/broxus/ton-locklift#readme",
  "dependencies": {
    "@broxus/contracts": "^1.0.3",
    "@eversdk/core": "^1.34.3",
    "@eversdk/lib-node": "^1.34.3",
    "bignumber.js": "^9.0.1",
    "commander": "^7.2.0",
    "directory-tree": "^2.2.7",
    "ejs": "^3.1.6",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "mocha": "^8.3.2",
    "superstruct": "^0.14.2",
    "tablemark": "^2.0.0",
    "ton-client-js": "^0.28.1",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/expect": "^24.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@types/underscore": "^1.11.4",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.7.2"
  }
}
